{
    "name": "askc",
    "version": "0.0.0",
    "description": "Kendo championship and grade management software",
    "author": {
        "name": "Kevin UNFRICHT",
        "email": "unfricht.kevin@hotmail.fr"
    },
    "license": "SEE LICENSE IN LICENSE.md",
    "homepage": "https://framagit.org/kendo/askc.git",
    "repository": {
        "type": "git",
        "url": "https://framagit.org/kendo/askc.git"
    },
    "build": {
        "appId": "com.askc.app",
        "mac": {
            "category": "public.app-category.sports"
        },
        "win": {
            "verifyUpdateCodeSignature": false
        },
        "publish": {
            "provider": "generic",
            "url": "https://framagit.org/"
        }
    },
    "scripts": {
        "dev": "electron-webpack dev",
        "compile": "electron-webpack",
        "build": "yarn compile && electron-builder",
        "build:dir": "yarn build --dir -c.compression=store -c.mac.identity=null",
        "build:win": "yarn build -w --x64 --ia32",
        "build:linux": "yarn build -l",
        "build:all": "yarn build -wl",
        "postinstall": "electron-builder install-app-deps"
    },
    "devDependencies": {
        "@tweenjs/tween.js": "^17.2.0",
        "animate.css": "^3.7.0",
        "axios": "^0.18.0",
        "bootstrap": "^4.3.1",
        "bootstrap-vue": "^2.0.1",
        "electron": "2.0.7",
        "electron-builder": "^20.39.0",
        "electron-store": "^5.0.0",
        "electron-webpack": "^2.6.1",
        "electron-webpack-vue": "^2.2.2",
        "faker": "^4.1.0",
        "file-loader": "^3.0.1",
        "jspdf": "^1.5.3",
        "jspdf-autotable": "^3.2.4",
        "luxon": "^1.17.3",
        "material-design-iconic-font": "^2.2.0",
        "node-sass": "^4.12.0",
        "papaparse": "^4.6.2",
        "sass-loader": "^7.1.0",
        "upath": "^1.0.5",
        "url-loader": "^2.2.0",
        "uuid": "^3.3.2",
        "v-mask": "^1.3.3",
        "vee-validate": "^2.1.3",
        "vue": "^2.6.10",
        "vue-breadcrumbs": "^1.1.2",
        "vue-good-table": "^2.16.0",
        "vue-infinite-loading": "^2.4.3",
        "vue-luxon": "^0.7.0",
        "vue-router": "^3.0.2",
        "vue-spinner": "^1.0.3",
        "vue-toasted": "^1.1.25",
        "vue2-datepicker": "^2.6.2",
        "vue2-filters": "^0.8.0-beta.1",
        "vuex": "^3.0.1",
        "vuex-map-fields": "^1.3.1",
        "webpack": "^4.26.1",
        "webpack-node-externals": "^1.7.2"
    },
    "resolutions": {
        "upath": "^1.0.5"
    },
    "dependencies": {
        "mariadb": "^2.1.1",
        "mysql": "^2.17.1",
        "sequelize": "^5.19.6",
        "source-map-support": "^0.5.9",
        "sqlite3": "^4.1.0",
        "umzug": "^2.2.0"
    }
}